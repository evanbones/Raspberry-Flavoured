{
  "multipart": [
    {
      "when": {
        "extend": "false"
      },
	  "apply": [
		  { "model": "environmental:block/cherry_hedge_post" },
		  { "model": "environmental:block/cherry_hedge_post_empty" }
      ]
    },
    {
      "when": {
        "extend": "true"
      },
      "apply": [
		  { "model": "environmental:block/cherry_hedge_extend" },
		  { "model": "environmental:block/cherry_hedge_extend_empty" }
      ]
    },
    {
      "when": {
        "north": "true"
      },
	  "apply": [
		  { "model": "environmental:block/cherry_hedge_side", "uvlock": true },
		  { "model": "environmental:block/cherry_hedge_side_empty", "uvlock": true }
      ]
    },
    {
      "when": {
        "east": "true"
      },
	  "apply": [
		  { "model": "environmental:block/cherry_hedge_side", "y": 90, "uvlock": true },
		  { "model": "environmental:block/cherry_hedge_side_empty", "y": 90, "uvlock": true }
      ]
    },
    {
      "when": {
        "south": "true"
      },
      "apply": [
		  { "model": "environmental:block/cherry_hedge_side", "y": 180, "uvlock": true },
		  { "model": "environmental:block/cherry_hedge_side_empty", "y": 180, "uvlock": true }
      ]
    },
    {
      "when": {
        "west": "true"
      },
      "apply": [
		  { "model": "environmental:block/cherry_hedge_side", "y": 270, "uvlock": true },
		  { "model": "environmental:block/cherry_hedge_side_empty", "y": 270, "uvlock": true }
      ]
    }
  ]
}